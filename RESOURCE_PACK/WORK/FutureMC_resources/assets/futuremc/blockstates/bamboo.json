{
    "multipart": [
        {
            "when": {
                "thick": "false"
            },
            "apply": {
                "model": "futuremc:bamboo_thin"
            }
        },
        {
            "when": {
                "thick": "true"
            },
            "apply": {
                "model": "futuremc:bamboo_thick"
            }
        },
        {
            "when": {
                "leaves": "small"
            },
            "apply": {
                "model": "futuremc:bamboo_leaves_0"
            }
        },
        {
            "when": {
                "leaves": "large"
            },
            "apply": {
                "model": "futuremc:bamboo_leaves_1"
            }
        }
    ]
}